<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.forzadroid.attentiontest">
    <uses-permission android:name="android.permission.VIBRATE"/>
    
    <application android:name="AttentionTestApplication" 
    	android:icon="@drawable/icon" android:label="@string/app_name">
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity android:name=".AttentionTest"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="DigitalSequenceActivity"
        	android:label="@string/main_digital_sequence">
        </activity>
        <activity android:name="DigitalSquareActivity"
        	android:label="@string/dig_square_title"
        	android:configChanges="orientation">
        </activity>
        <activity android:name="RecordsActivity"
        	android:label="@string/main_records">
        </activity>
    </application>
    
</manifest>